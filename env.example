# Environment Variables for Homelab Monitoring Stack
# Copy this file to .env and update with your actual values

# Grafana Configuration
GRAFANA_PASSWORD=your_secure_password_here

# Synology NAS Configuration
SYNOLOGY_IP=192.168.1.131

# Mac Mini Configuration
MAC_MINI_IP=192.168.1.147

# Octopus Energy Configuration
OCTOPUS_API_KEY=your_octopus_api_key_here
OCTOPUS_ACCOUNT_NUMBER=A-XXXXXXXX

# Octopus Energy Meter Details (optional - will auto-detect if not provided)
OCTOPUS_ELECTRIC_MPAN=your_electric_mpan_here
OCTOPUS_GAS_MPRN=your_gas_mprn_here
OCTOPUS_ELECTRIC_SERIAL=your_electric_serial_here
OCTOPUS_GAS_SERIAL=your_gas_serial_here

# Example Database Configuration (for examples/example-with-exporters.yml)
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_secure_db_password
POSTGRES_DB=your_database_name
